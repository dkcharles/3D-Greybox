            <ui:FloatField label="Position" value="0.5" name="LoopPosition" />
            <ui:EnumField label="Direction" name="LoopDirection" />
            <ui:EnumField label="Mode" name="LoopMode" />
        </ui:VisualElement>
        <ui:VisualElement name="Bridge" class="panel-secondary column">
            <ui:Label tabindex="-1" text="Bridge Edges" parse-escape-sequences="true" display-tooltip-when-elided="true" name="Title" class="h3" />
            <ui:IntegerField label="Rotation Offset" value="0" name="BridgeRotationOffset" />
            <ui:Toggle label="Reverse Order" name="BridgeReverseOrder" value="false" />
            <ui:Toggle label="Use Full Borders" name="BridgeUseFullBorders" value="true" />
        </ui:VisualElement>
        <ui:VisualElement name="Connect" class="panel-secondary column">
            <ui:Label tabindex="-1" text="Connect Edges" parse-escape-sequences="true" display-tooltip-when-elided="true" name="Title" class="h3" />
            <ui:FloatField label="Position" value="0.5" name="ConnectionPosition" />
            <ui:EnumField label="Direction" name="ConnectionDirection" />
            <ui:EnumField label="Mode" name="ConnectionMode" />
        </ui:VisualElement>
        <ui:VisualElement name="ExtrudeFaces" class="panel-secondary column">
            <ui:Label tabindex="-1" text="Extrude Faces" parse-escape-sequences="true" display-tooltip-when-elided="true" name="Title" class="h3" />
            <ui:FloatField label="Distance" value="0.5" name="ExtrudeFacesDistance" />
            <ui:EnumField label="Method" name="ExtrudeFacesMethod" />
            <ui:EnumField label="Space" name="ExtrudeFacesSpace" />
            <ui:EnumField label="Axis" name="ExtrudeFacesAxis" />
        </ui:VisualElement>
        <ui:VisualElement name="InsetFaces" class="panel-secondary column">
            <ui:Label tabindex="-1" text="Inset Faces" parse-escape-sequences="true" display-tooltip-when-elided="true" name="Title" class="h3" />
            <ui:FloatField label="Distance" value="0.3" name="InsetFacesDistance" />
        </ui:VisualElement>
        <ui:VisualElement name="ConformNormals" class="panel-secondary column">
            <ui:Label tabindex="-1" text="Conform Normals" parse-escape-sequences="true" display-tooltip-when-elided="true" name="Title" class="h3" />
            <ui:Toggle label="Other Direction" name="ConformNormalsOtherDirection" value="false" />
        </ui:VisualElement>
        <ui:VisualElement name="SeparateFaces" class="panel-secondary column">
            <ui:Label tabindex="-1" text="Separate Faces" parse-escape-sequences="true" display-tooltip-when-elided="true" name="Title" class="h3" />
            <ui:Toggle label="New Object" name="SeparateFacesCreateNewObject" value="false" />
            <ui:Toggle label="Duplicate" name="SeparateFacesDuplicateFaces" value="false" />
        </ui:VisualElement>
        <ui:VisualElement name="RemoveElements" class="panel-secondary column">
            <ui:Label tabindex="-1" text="Remove Elements" parse-escape-sequences="true" display-tooltip-when-elided="true" name="Title" class="h3" />
            <ui:FloatField label="Extrude Distance" value="1.0" name="RemoveExtrudeDistance" />
        </ui:VisualElement>
        <ui:VisualElement name="FillHole" class="panel-secondary column">
            <ui:Label tabindex="-1" text="Fill Hole" parse-escape-sequences="true" display-tooltip-when-elided="true" name="Title" class="h3" />
            <ui:Toggle label="Use Entire Border" name="FillEntirePath" value="true" />
        </ui:VisualElement>
        <ui:VisualElement name="BevelElements" class="panel-secondary column">
            <ui:Label tabindex="-1" text="Bevel Elements" parse-escape-sequences="true" display-tooltip-when-elided="true" name="Title" class="h3" />
            <ui:FloatField label="Distance" value="0.2" name="BevelDistance" />
            <ui:Toggle label="Perimeter Only" name="BevelPerimeterOnly" value="false" />
        </ui:VisualElement>
        <ui:VisualElement name="OffsetElements" class="panel-secondary column">
            <ui:Label tabindex="-1" text="Offset Elements" parse-escape-sequences="true" display-tooltip-when-elided="true" name="Title" class="h3" />
            <ui:EnumField label="Relative To" name="OffsetCoordinateSpace" />
            <ui:Vector3Field label="Offset Amount" name="OffsetVector" />
        </ui:VisualElement>
        <ui:VisualElement name="MirrorObjects" class="panel-secondary column">
            <ui:Label tabindex="-1" text="Mirror Objects" parse-escape-sequences="true" display-tooltip-when-elided="true" name="Title" class="h3" />
            <ui:Toggle label="X Axis" name="MirrorX" value="true" />
            <ui:Toggle label="Y Axis" name="MirrorY" value="false" />
            <ui:Toggle label="Z Axis" name="MirrorZ" value="false" />
            <ui:Toggle label="Duplicate" name="MirrorDuplicate" value="false" />
        </ui:VisualElement>
        <ui:VisualElement name="ApplyTransform" class="panel-secondary column">
            <ui:Label tabindex="-1" text="Apply Transform" parse-escape-sequences="true" display-tooltip-when-elided="true" name="Title" class="h3" />
            <ui:Toggle label="Apply Position" name="ApplyPosition" value="true" />
            <ui:Toggle label="Apply Rotation" name="ApplyRotation" value="true" />
            <ui:Toggle label="Apply Scale" name="ApplyScale" value="true" />
        </ui:VisualElement>
        <ui:VisualElement name="ConformObjectNormals" class="panel-secondary column">
            <ui:Label tabindex="-1" text="Conform Object Normals" parse-escape-sequences="true" display-tooltip-when-elided="true" name="Title" class="h3" />
            <ui:Toggle label="Other Direction" name="ConformObjectNormalsOtherDirection" value="false" />
